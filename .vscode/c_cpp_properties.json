{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/arduino",
                "C:/Program Files (x86)/Arduino/libraries/**",
                "${env:USERPROFILE}/OneDrive/Documents/Arduino/libraries/**",
                "${env:USERPROFILE}/OneDrive/Documentos/Arduino/libraries/**",
                "${env:USERPROFILE}/Documents/Arduino/libraries/**",
                "${env:USERPROFILE}/Documentos/Arduino/libraries/**",
                "${env:OneDrive}/Documents/Arduino/libraries/**",
                "${env:OneDrive}/Documentos/Arduino/libraries/**"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/.browse.db"
            },
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.26100.0",
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-msvc-x64"
        },
        {
            "name": "Arduino",
            "includePath": [
                "${workspaceFolder}/arduino/**",
                "C:/Program Files (x86)/Arduino/hardware/arduino/avr/cores/arduino",
                "C:/Program Files (x86)/Arduino/hardware/arduino/avr/variants/standard",
                "C:/Program Files/Arduino/hardware/arduino/avr/cores/arduino",
                "C:/Program Files/Arduino/hardware/arduino/avr/variants/standard",
                "${env:USERPROFILE}/AppData/Local/Arduino15/packages/arduino/hardware/avr/*/cores/arduino",
                "${env:USERPROFILE}/AppData/Local/Arduino15/packages/arduino/hardware/avr/*/variants/standard",
                "${env:USERPROFILE}/AppData/Local/Arduino15/packages/arduino/tools/avr-gcc/*/avr/include",
                "C:/Program Files (x86)/Arduino/libraries/**",
                "C:/Program Files/Arduino/libraries/**",
                "${env:USERPROFILE}/OneDrive/Documents/Arduino/libraries/**",
                "${env:USERPROFILE}/OneDrive/Documentos/Arduino/libraries/**",
                "${env:USERPROFILE}/Documents/Arduino/libraries/**",
                "${env:USERPROFILE}/Documentos/Arduino/libraries/**",
                "${env:OneDrive}/Documents/Arduino/libraries/**",
                "${env:OneDrive}/Documentos/Arduino/libraries/**"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/.browse.db"
            },
            "defines": [
                "ARDUINO=10819",
                "ARDUINO_AVR_UNO",
                "ARDUINO_ARCH_AVR",
                "__AVR_ATmega328P__",
                "F_CPU=16000000L"
            ],
            "compilerPath": "${env:USERPROFILE}/AppData/Local/Arduino15/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/avr-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": false
}